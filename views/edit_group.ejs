<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Edit Group</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.5.0/css/bulma.min.css">
  </head>
  <center>
  <body>
    <section class="section">
    <div class="container">
  <h1 class="title">Edit Group</h1>
    <h2>
      <%dataG.forEach(function(d){ %>
      <form action="/groups/edit/<%=d.id%>" method="post">
        Group Name:<br>
        <input type="text" name="group" value="<%=d.name_of_group%>"><br><br>
        <button class="button is-primary" type="submit" name="button">Save Changes</button><br>
      </form>
    </h2>
    <%})%>
  </div>
</section>
  </center>
  </body>
</html>

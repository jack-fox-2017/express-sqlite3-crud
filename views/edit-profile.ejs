<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit profile</title>
  </head>
  <body>
    <h1>Edit profile</h1>
    <% let d = data[0] %>
    <form action="/profiles/edit/<%= d.id %>" method="post">
      <input type="text" name="date_of_birth" placeholder="Street Address" value="<%= d.date_of_birth %>"><br>
      <input type="text" name="age" placeholder="City" value="<%= d.age %>"><br>
      <input type="text" name="hobby" placeholder="State / Province" value="<%= d.hobby %>"><br><br>
      <input type="submit" value="Submit">
    </form>
  </body>
</html>

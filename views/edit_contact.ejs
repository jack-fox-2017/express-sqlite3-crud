<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>edit contact</title>
  </head>
  <body>
    <h1>===Edit Contacts===</h1>
    <h2>
      <%dataC.forEach(function(d){ %>
      <form action="/contact/edit/<%=d.id%>" method="post">
        Name:<br>
        <input type="text" name="name" value="<%=d.name%>"><br>
        Company:<br>
        <input type="text" name="company" value="<%=d.company%>"> <br>
        Phone Number:<br>
        <input type="text" name="telp_number" value="<%=d.telp_number%>"><br>
        Email:<br>
        <input type="text" name="email" value="<%=d.email%>"><br>
        <br>
        <button type="submit" name="button">Submit</button> <br>
        <button type="submit" name="button">Submit</button>
      </form>
    </h2>
    <%})%>
  </body>
</html>

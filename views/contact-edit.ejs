<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <%- include part/css %>
  </head>
  <body>
    <%- include part/menu %>

    <div class="col-md-8 col-md-offset-2">
      <form class="form-inline" action="/contacts/<%= edit.id%>/edit" method="post">
        <input type="text" name="name" value="<%= edit.name%>" class="form-control">
        <input type="text" name="company" value="<%= edit.company%>" class="form-control">
        <input type="text" name="phone"  value="<%= edit.phone%>" class="form-control">
        <input type="text" name="email"  value="<%= edit.email%>" class="form-control">
        <button type="submit" name="button" class="btn btn-info"><span class="glyphicon glyphicon-send"></span> &nbsp; Edit Contact</button>
      </form>
    </div>
  </body>
</html>

<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <form action="/addresses/edit/<%= data[0].id %>" method="post">
      <input type="text" name="street" placeholder="Street" value="<%= data[0].street %>">
      <input type="text" name="city" placeholder="city" value="<%= data[0].city %>">
      <select name="contactId">
        <% data2.forEach(d => { %>
        <option value="<%= d.id %>"><%= d.name %></option>
        <% }) %>
      </select>
      <button type="submit" name="submit">Submit</button>
    </form>
  </body>
</html>

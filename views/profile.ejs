<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>profile</title>
</head>
<body>
  <h1>profile</h1>
  <table border="1">
    <tr>
      <th>ID</th>
      <th>username</th>
      <th>password</th>
    </tr>
    <% data.forEach (d =>{%>
      <tr>
        <td>
          <%= d.no %>
        </td>
        <td>
          <%= d.username %>
        </td>
        <td>
          <%= d.password %>
        </td>
        <td><a href="/profile/edit/<%= d.no %>"><button type="button" name="button">Edit</button></a></td>
        <td><a href="/profile/delete/<%= d.no %>"><button type="button" name="button">Delete</button></a></td>
      </tr>
      <% }) %>
  </table>
  <br>
  <left>
    <form action="/profile" method="post">
      <label><b>user name<b></label>
      <input type="text" name="username" placeholder="please input user name">
      <label><b>password<b></label>
      <input type="text" name="password" placeholder="please input password">
      <button type="submit" name="submit">Save </button>
    </form><br>
  </left>
  <br>
    <a href="/">back to home</a>
</body>
</html>
